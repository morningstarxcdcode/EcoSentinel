{
  "name": "ecosentinel",
  "version": "1.0.0",
  "description": "AI-Powered Environmental Monitoring & Action Platform - Revolutionary environmental intelligence for climate action",
  "author": "EcoSentinel Team",
  "contributors": [
    {
      "name": "morningstarxcdcode",
      "email": "morningstar.xcd@gmail.com",
      "url": "https://github.com/morningstarxcdcode"
    }
  ],
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:check": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:backend": "jest server/__tests__",
    "test:integration": "jest --testPathPattern=tests/integration",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "api": "node server/index.js",
    "server": "node server/index.js",
    "websocket": "node server/websocket-server.js",
    "ai-service": "python3 ai/main.py",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\" \"npm run websocket\"",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "db:reset": "node scripts/reset.js",
    "docker:build": "docker build -t ecosentinel .",
    "docker:run": "docker run -p 3000:3000 ecosentinel",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "setup": "./scripts/setup.sh",
    "clean": "rm -rf .next node_modules/.cache",
    "analyze": "ANALYZE=true npm run build",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:components": "jest --testPathPattern=components",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e",
    "prepare": "husky install",
    "postinstall": "npm run type-check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/username/ecosentinel.git"
  },
  "homepage": "https://github.com/morningstarxcdcode/ecosentinel#readme",
  "bugs": {
    "url": "https://github.com/morningstarxcdcode/ecosentinel/issues",
    "email": "morningstar.xcd@gmail.com"
  },
  "keywords": [
    "climate",
    "environment",
    "ai",
    "hackathon",
    "sustainability",
    "monitoring"
  ],
  "dependencies": {
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@vercel/analytics": "1.1.1",
    "axios": "^1.10.0",
    "bcryptjs": "2.4.3",
    "chart.js": "4.4.0",
    "class-variance-authority": "0.7.0",
    "clsx": "2.0.0",
    "concurrently": "^8.2.2",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "7.1.5",
    "framer-motion": "10.16.16",
    "helmet": "7.1.0",
    "joi": "17.11.0",
    "jsonwebtoken": "9.0.2",
    "lucide-react": "0.292.0",
    "next": "^14.2.30",
    "pg": "8.11.3",
    "prom-client": "14.2.0",
    "react": "18.2.0",
    "react-chartjs-2": "5.2.0",
    "react-dom": "18.2.0",
    "redis": "4.6.11",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "tailwind-merge": "2.2.0",
    "tailwindcss": "3.3.6",
    "typescript": "5.3.3",
    "winston": "3.11.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.1",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "2.4.6",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "9.0.5",
    "@types/pg": "8.10.9",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.2",
    "playwright": "1.40.1",
    "postcss": "8.4.32",
    "prettier": "^3.1.1"
  },
  "license": "MIT"
}
